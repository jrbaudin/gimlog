{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": [
            false
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "curly": true,
        "eofline": true,
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "ter-func-call-spacing": [
            true,
            "never"
        ],
        "handle-callback-err": [
            true,
            "^(err|error|anySpecificError)$"
        ],
        "new-parens": true,
        "no-arg": true,
        "no-conditional-assignment": true,
        "no-constant-condition": true,
        "no-control-regex": true,
        "no-debugger": true,
        "no-duplicate-case": true,
        "no-empty-character-class": true,
        "no-eval": true,
        "no-ex-assign": true,
        "no-extra-boolean-cast": true,
        "no-switch-case-fall-through": true,
        "no-inner-declarations": [
            true,
            "functions"
        ],
        "no-invalid-regexp": true,
        "ter-no-irregular-whitespace": [
            true
        ],
        "label-position": true,
        "indent": [
            true,
            "spaces"
        ],
        "no-multi-spaces": [
            true
        ],
        "no-consecutive-blank-lines": [
            true,
            2
        ],
        "no-unused-new": true,
        "no-construct": true,
        "no-duplicate-variable": true,
        "no-regex-spaces": true,
        "ter-no-sparse-arrays": [
            true
        ],
        "no-string-throw": true,
        "no-trailing-whitespace": true,
        "no-unexpected-multiline": true,
        "no-unsafe-finally": true,
        "no-unused-variable": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "no-var-requires": false,
        "one-variable-per-declaration": [
            true,
            "ignore-for-loop"
        ],
        "object-literal-key-quotes": [
            true,
            "consistent"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "no-extra-semi": true,
        "space-in-parens": [
            true,
            "never"
        ],
        "comment-format": [
            true,
            "check-space"
        ],
        "use-isnan": true,
        "valid-typeof": true,
        "max-line-length": [
            true,
            140
        ],
        "member-access": true,
        "member-ordering": [
            true,
            "public-before-private",
            "static-before-instance",
            "variables-before-functions"
        ],
        "no-console": [
            true,
            "log",
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ]
    },
    "rulesDirectory": []
}